      "/NPS-LIC/": {
        "get": {
          "tags": [
            "NPS-LIC"
          ],
          "summary": "South African Motor vehicle licence disk barcode & VIN decoder",
          "description": "This is the official NPS-LIC interface third party intergrators can use to intergrate and lookup the NP Tracker database of suspect vehicles into their apps and webservises.   Our API includes a VIN decoder and checker that includes an image of the make + model of the vehicle. Contact us if you need lookups done from third party sources aswell and we can intergrate it into a solution for you.<br> Supported Barcodes:<br> <ul> <li>South African motor vehicle licence disk</li> <li>South African vehicle registration certificate</li> </ul>\nCosts Free",
          "operationId": "NPS-LIC",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "scan",
              "in": "query",
              "description": "PDF417 barcode string ie. %MVL... (Note that the submitted HTTP GET need to be URI encoded iaw replace % with %25 ect.)",
              "required": false,
              "type": "string"
            },
            {
              "name": "vin",
              "in": "query",
              "description": "VIN string to test (only 17 character VIN's are supported).",
              "required": false,
              "type": "string"
            },
            {
              "name": "token",
              "in": "query",
              "description": "API-Key (Available on https://npscloud.co.za Login -> NPS-API)",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/NPS-LIC"
                }
              }
            },
            "400": {
              "description": "Invalid status value"
            }
          }
        }
      },
