      "/NPS-ANPR/": {
        "get": {
          "tags": [
            "NPS-ANPR"
          ],
          "summary": "Add detected plates to your NPS-CLOUD account",
          "description": "This endpoint are used to add the plates detected from your LRP/ANPR system to your NPS-CLOUD (https://npscloud.co.za) account, from your NPS-CLOUD account notifications, triggers and reports can be setup, contact us if you need lookups done from third party sources aswell and we can intergrate it into a solution for you.\nCosts (1 credit)",
          "operationId": "NPS-ANPR",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "plates",
              "in": "query",
              "description": "Multiple detected plate candidates to be seperated by :<br> ie. ABC123GP:ABCL23GP:A8C123GP...",
              "required": true,
              "type": "string"
            },
            {
              "name": "token",
              "in": "query",
              "description": "ANPR Token (Available on https://npscloud.co.za Login -> ANPR Feeds)",
              "required": true,
              "type": "string"
            },
            {
              "name": "event",
              "in": "query",
              "description": "Event ID used to classify plate groups together.",
              "required": false,
              "type": "string"
            },
            {
              "name": "confidence",
              "in": "query",
              "description": "Multiple detected plate confidence (0-100) to be seperated by :<br> ie. 99:80:60...",
              "required": false,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/NPS-ANPR"
                }
              }
            },
            "400": {
              "description": "Invalid status value"
            }
          }
        }
      },
      "/NPS-SHARE/": {
        "get": {
          "tags": [
            "NPS-SHARE"
          ],
          "summary": "Share incident reports between platforms",
          "description": "This endpoint are used to share vehicle lookout reports between partnered sites contact us to setup an account.\nCosts (Free)",
          "operationId": "NPS-SHARE",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "token",
              "in": "query",
              "description": "Application Token (Available from info@nptracker.co.za)",
              "required": true,
              "type": "string"
            },
            {
              "name": "message",
              "in": "query",
              "description": "URI encoded message to share and to lookup plates against the NP Tracker suspect and suspicious vehicle database.",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/NPS-SHARE"
                }
              }
            },
            "400": {
              "description": "Invalid status value"
            }
          }
        }
      },
