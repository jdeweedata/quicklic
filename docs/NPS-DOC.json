    "paths": {
      "/NPS-API/": {
        "get": {
          "tags": [
            "NPS-API"
          ],
          "summary": "SADC Vehicle registration lookup",
          "description": "This is the official NPS-API interface third-party integrators can use to integrate and lookup the NP Tracker database of suspect vehicles into their apps and web services.  Contact us if you need lookups done from third party sources aswell and we can intergrate it into a solution for you.\nCosts (1 credit)<br>For bulk enquiries use our [server interface](http://api.nptracker.co.za/phpexample.html)",
          "operationId": "NPS-API",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "cmd",
              "in": "query",
              "description": "Commands to execute {ADD/DEMO/NPS/MUTE/UNMUTE/PING}<br> <b>ADD</b> - Add a new lookout<br> <b>DEMO/PING</b> - Test your script is working<br> <b>NPS</b> - To do a SADC vehicle registration lookup<br> <b>MUTE</b> - Do not recieve reports for this registration<br> <b>UNMUTE</b> - Unmute a previously muted registration number<br>",
              "required": true,
              "type": "string"
            },
            {
              "name": "str",
              "in": "query",
              "description": "The registration number to test/mute/unmute or lookout to add<br>ex. <b>ABC123GP</b>",
              "required": true,
              "type": "string"
            },
            {
              "name": "token",
              "in": "query",
              "description": "API-Key (Available on https://npscloud.co.za Login -> NPS-API)",
              "required": true,
              "type": "string"
            },
            {
              "name": "id",
              "in": "query",
              "description": "Query identifier",
              "required": false,
              "type": "integer"
            },
            {
              "name": "trim",
              "in": "query",
              "description": "Trim message to length specified",
              "required": false,
              "type": "integer"
            },
            {
              "name": "history",
              "in": "query",
              "description": "Only show lookout reports less than specified months",
              "required": false,
              "type": "integer"
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/NPS-API"
                }
              }
            },
            "400": {
              "description": "Invalid status value"
            }
          }
        }
      },
      "/NPS-DL/": {
        "get": {
          "tags": [
            "NPS-DL"
          ],
          "summary": "South African Drivers licence barcode decoder",
          "description": "This is the official NPS-DL (Drivers Licence barcode decoder) interface for intergrators to use, contact us if you need lookups done from third party sources aswell and we can intergrate it into a solution for you.\nCosts (5 credits)",
          "operationId": "NPS-DL",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "scan",
              "in": "query",
              "description": "The HEX encoded binary string of the PDF417 decoded South African drivers licence. ie. 019b0945000025...",
              "required": true,
              "type": "string"
            },
            {
              "name": "token",
              "in": "query",
              "description": "API-Key (Available on https://npscloud.co.za Login -> NPS-API)",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/NPS-DL"
                }
              }
            },
            "400": {
              "description": "Invalid status value"
            }
          }
        }
      },
      "/NPS-DLI/": {
        "get": {
          "tags": [
            "NPS-DLI"
          ],
          "summary": "South African Drivers licence photo decoder",
          "description": "This is the NPS-DLI (Drivers Licence photo decoder) interface for intergrators to use, it accepts the encrypted image from the decrypted barcode and display the png image (if all sounds greek to you rather use the NPS-DL endpoint).\nCosts (6 credits)",
          "operationId": "NPS-DLI",
          "produces": [
            "image/png"
          ],
          "parameters": [
            {
              "name": "scan",
              "in": "query",
              "description": "The HEX encoded binary string of the image.",
              "required": true,
              "type": "string"
            },
            {
              "name": "token",
              "in": "query",
              "description": "API-Key (Available on https://npscloud.co.za Login -> NPS-API)",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/NPS-DL"
                }
              }
            },
            "400": {
              "description": "Invalid status value"
            }
          }
        }
      },
